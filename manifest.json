{
  "name": "Gyoogle",
  "version": "0.0.2",
  "author": "pastak",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "applications": {
    "gecko": {
      "id": "gyoogle@pastak.net"
    }
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*", "*://www.google.co.jp/search*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
